    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.8.0",
    "jsdom": "24.0.0",
    "json-server": "1.0.0-beta.1",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
